def convert_to_fahrenheit(degrees_celsius):
    print(degrees_celsius * (9 / 5) + 32)

def convert_to_celsius(degrees_fahrenheit):
    print((degrees_fahrenheit - 32) * (5 / 9)) 

def main():
    '''Function which executes the program'''
    celsius = float(input('Insert the temperature in Celsius: '))
    fahrenheit = float(input('Insert the temperature in Fahrenheit: '))
    convert_to_fahrenheit(celsius)
    convert_to_celsius(fahrenheit)

if __name__ == '__main__':
    main()